{
  "version": 2,
  "functions": {
    "api/main.py": {
      "runtime": "@vercel/python",
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/main"
    }
  ],
  "build": {
    "env": {
      "PYTHONPATH": "."
    }
  }
}
